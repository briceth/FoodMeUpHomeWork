{
	"presets": [
		"react",
		[
			"env",
			{
				"targets": {
					"browsers": ["last 2 versions", "safari >= 7"]
				},
				"modules": false
			}
		],
		"stage-2"
	],
	"plugins": [
		"transform-decorators-legacy",
		"transform-decorators",
		"add-module-exports",
		"babel-plugin-styled-components"
	]
}
